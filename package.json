{
  "name": "next-boilerplate",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "buildci": "CI=true next build",
    "prestart": "npm run build",
    "start": "next start",
    "lint": "next lint",
    "lintes": "eslint --ext .tsx,.ts .",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "type:check": "tsc",
    "validate": "npm-run-all --parallel type:check format:check lintes test:ci buildci",
    "validate-nobuild": "npm-run-all --parallel type:check format:check lintes test:ci",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "chromatic": "chromatic --exit-zero-on-changes",
    "preanalyze": "npm run build",
    "analyze": "npx source-map-explorer .next/static/**/*.js",
    "depcheck": "npx dependency-cruiser --config .dependency-cruiser.js src",
    "depcheck:graph": "npx dependency-cruiser --include-only '^src' --output-type dot src | dot -T svg > dependencygraph.svg",
    "cloc": "npx cloc src",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "gql:codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^0.1.1",
    "@react-icons/all-files": "^4.1.0",
    "@sentry/nextjs": "^6.15.0",
    "firebase": "^9.5.0",
    "graphql": "^16.1.0",
    "next": "12.0.4",
    "next-pwa": "^5.4.1",
    "next-seo": "^4.28.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.34.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@graphql-codegen/typescript-react-query": "^3.2.2",
    "@storybook/addon-a11y": "^6.4.0",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addon-essentials": "^6.4.0",
    "@storybook/addon-links": "^6.4.0",
    "@storybook/react": "^6.4.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.2.1",
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "27.0.6",
    "babel-loader": "^8.2.3",
    "chromatic": "^6.0.6",
    "cypress": "^9.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.0",
    "eslint-plugin-testing-library": "4.11.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.6",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "prettier": "2.5.0",
    "start-server-and-test": "^1.14.0",
    "storybook-addon-designs": "^6.2.0",
    "tailwindcss": "^2.2.19",
    "typescript": "4.5.2"
  }
}
